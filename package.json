{
  "name": "release-cli",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.6.12",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "release": "./bin/release.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "npm run build --watch",
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "start": "tsx src/cli/run.ts",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "release": "",
    "publish": "npm run release && npm publish"
  },
  "dependencies": {
    "c12": "^1.4.2",
    "cac": "^6.7.14",
    "picocolors": "^1.0.0",
    "prompts": "^2.4.2",
    "semver": "^7.5.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.4",
    "@types/node": "^20.5.0",
    "@types/prompts": "^2.4.4",
    "@types/semver": "^7.5.0",
    "eslint": "^8.42.0",
    "pnpm": "^8.6.12",
    "typescript": "^5.1.3"
  }
}
